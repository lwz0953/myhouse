<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.myhouse.biz.mapper.UserMapper">


    

    
    <insert id="save">
      insert into user (
                name,
                phone,
                email,
                aboutme,
                passwd,
                avatar,
                type,
                create_time,
                enable
                )
                values(
                #{name},
                #{phone},
                #{email},
                #{aboutme},
                #{passwd},
                #{avatar},
                #{type},
                #{createTime},
                #{enable}
                )
      
    
    </insert>
    

   

</mapper> 